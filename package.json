    "dependencies": {
        "body-parser": "1.19.0",
        "concurrently": "^5.3.0",
        "cookie-parser": "~1.4.4",
        "cors": "2.8.5",
        "debug": "~2.6.9",
        "dotenv": "8.2.0",
        "express": "~4.16.1",
        "moment": "^2.29.1",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "nodemon": "1.18.4"
    },
    "keywords": []
}